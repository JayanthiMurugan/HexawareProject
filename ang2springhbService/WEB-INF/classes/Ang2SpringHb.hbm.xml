<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name ="com.hex.vo.Product" table="PRODUCT" lazy="false">
<id column="PRODUCTID" name="productid" type="java.lang.Integer">
<generator class="assigned"/>
</id>
<property column="NAME" length="20" name="name" type="java.lang.String"/>
<property column="CATEGORYID" length="11" name="categoryid" type="java.lang.Integer"/>
<property column="DESCRIPTION" length="50" name="description" type="java.lang.String"/>
<property column="ACTION" length="20" name="action" type="java.lang.String"/>
<property column="PRICE" length="11" name="price" type="java.lang.Integer"/>
<property column="QUANTITY" length="11" name="quantity" type="java.lang.Integer"/>
<property column="CREATEDDATE" length="19" name="createddate" type="java.util.Date"/>
<property column="CREATEDBY" length="30" name="createdby" type="java.lang.String"/>
<property column="MODIFIEDDATE" length="19" name="modifieddate" type="java.util.Date"/>
<property column="MODIFIEDBY" length="30" name="modifiedby" type="java.lang.String"/>
</class>


</hibernate-mapping>
